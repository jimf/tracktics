  },
  "homepage": "https://github.com/jimf/tracktics",
  "devDependencies": {
    "browserify-istanbul": "^0.2.1",
    "coveralls": "^2.11.2",
    "jasmine-core": "^2.3.4",
    "jquery": "^2.1.4",
    "jshint": "^2.7.0",
    "karma": "^0.12.36",
    "karma-browserify": "^4.2.1",
    "karma-coverage": "^0.4.2",
    "karma-jasmine": "^0.3.5",
